{
	"name": "template-svelte-ts",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"dev:docker": "docker-compose -f docker-compose.dev.yml up --build",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
		"test:watch": "npm run test -- --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.67",
		"@sveltejs/adapter-static": "^1.0.0-next.30",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.14.0",
		"@testing-library/svelte": "^3.1.1",
		"@types/jest": "^27.0.0",
		"@types/testing-library__jest-dom": "^5.14.3",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.23.0",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jest": "^27.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.49.9",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.9.4",
		"ts-jest": "^27.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"bootstrap": "^5.1.3",
		"bootstrap-icons": "^1.8.1"
	},
	"type": "module"
}
